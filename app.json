{
   "expo": {
      "name": "Zalo",
      "slug": "Zalo",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "src/assets/images/zalo_arc.png",
      "userInterfaceStyle": "light",
      "splash": {
         "image": "src/assets/images/splash_image.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "bundleIdentifier": "fit.se.zalomobile",
         "googleServicesFile": "./GoogleService-Info.plist",
         "supportsTablet": true
      },
      "android": {
         "googleServicesFile": "./google-services.json",
         "package": "fit.se.zalomobile",
         "adaptiveIcon": {
            "foregroundImage": "src/assets/images/zalo_arc.png",
            "backgroundColor": "#ffffff"
         }
      },
      "web": {
         "favicon": "src/assets/images/zalo_arc.png"
      },
      "plugins": [
         "@react-native-firebase/app",
         "@react-native-firebase/auth",
         [
            "expo-build-properties",
            {
               "android": {
                  "compileSdkVersion": 33,
                  "targetSdkVersion": 33,
                  "buildToolsVersion": "33.0.0"
               },
               "ios": {
                  "useFrameworks": "static",
                  "deploymentTarget": "13.0"
               }
            }
         ]
      ],
      "extra": {
         "eas": {
            "projectId": "b811f300-26a6-45ff-bfbc-9614825f87fe"
         }
      }
   }
}
